{
  "name": "tsnv",
  "version": "0.0.0",
  "description": "Modern build toolkit for React Native libraries",
  "license": "MIT",
  "author": "Geunhyeok Lee <dev.ghlee@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/leegeunhyeok/tsnv.git"
  },
  "bin": "bin.js",
  "files": [
    "bin.js",
    "dist"
  ],
  "type": "module",
  "main": "./dist/config.cjs",
  "module": "./dist/config.js",
  "types": "./dist/config.d.cjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/config.d.js",
        "default": "./dist/config.js"
      },
      "require": {
        "types": "./dist/config.d.cjs",
        "default": "./dist/config.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepack": "yarn build",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint --type-aware --deny-warnings",
    "lint:fix": "oxlint --type-aware --deny-warnings --fix",
    "fmt": "oxfmt",
    "test": "vitest --run",
    "test:e2e": "vitest --run --config vitest.e2e.config.ts",
    "build": "tsdown"
  },
  "dependencies": {
    "c12": "^3.3.3",
    "empathic": "^2.0.0",
    "obug": "^2.1.1",
    "rolldown": "1.0.0-beta.60",
    "rolldown-plugin-dts": "0.21.2",
    "tsdown": "0.20.0-beta.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "es-toolkit": "^1.43.0",
    "oxfmt": "^0.24.0",
    "oxlint": "^1.39.0",
    "oxlint-tsgolint": "^0.11.1",
    "picocolors": "^1.1.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17",
    "zx": "^8.8.5"
  },
  "dependenciesMeta": {
    "@typescript/native-preview": {},
    "oxfmt": {
      "unplugged": true
    },
    "oxlint": {
      "unplugged": true
    },
    "oxlint-tsgolint": {
      "unplugged": true
    },
    "typescript": {}
  },
  "peerDependencies": {
    "@typescript/native-preview": "*",
    "typescript": "*"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "packageManager": "yarn@4.12.0"
}
